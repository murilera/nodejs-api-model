{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-unused-vars": 0,
    "no-useless-escape": 0,
    "space-before-function-paren": 0,
    "prefer-const": 0
  }
}
